{
    #Example colors
    cNATSLightBlue: [ 29, 170, 225 ],
    cNATSDarkGreen: [52, 165, 116 ],
    cNATSDarkBlue: [ 55, 92, 147 ],
    cNATSLightGreen: [ 141, 198, 63 ],
    
    orientation: landscape,
    
    borderleft: 35,
    borderright: 35,
    bordertop: 10,
    borderbottom: 50,
    
    backgroundTLLogo: wave_top.png,
    backgroundBRLogo: wave_bottom.png,
    backgroundTLWidth: 250,
    backgroundTLHeight: 70,
    backgroundBRWidth: 400,
    backgroundBRHeight: 70,
    
    #Page header properties
    headerFont: helvetica,   
    headerFontSize: 24,
    headerFontColor: [0,0,0], 
    headerYOffset: -6,
    headerHeight: 60,
    headerLogoHeight: 40, 
    headerLogo: NATS.png ,
        
    #Footer
    footerFont: helvetica,   
    footerFontSize: 8,
    footerLineSpacingRel: 0.2, 
    footerFontColor: [0,0,0], 
    footerWidth: 260,
    footerDate: true,
    footerPadding: {
        "pad.top": 5,
        "pad.bottom": 15,
        "pad.left": 35,
        "pad.right": 6,
    },
    
    #Number of lines and border between them
    laneborder: 15,
    lanes: 4,
    
    #Adjust relative lane width
    #1.0 is default. Should balance out to number of lanes
    laneWidthRel: [ 1.0, 1.0, 1.0, 1.0 ],


    #Block title properties
    titleFont: helvetica,    
    titleFontSize: 13,
    titleFontColor: [0,0,0], 
    titleLineSpacingRel: 0.2,
    titleParagraphSpacingRel: 0.3,
    
    #Logo to be used under the block title
    underLineLogo: underline.png, 
    underlineHeightRel: 0.2, 
    underlineBorderRel: 0.1,
    
   
    #Should bullets be shown on items
    bullets: true,
    #Bullet symbol and format
    bullet: "\u2022",
    bulletSizeRel: 1.3,
    bulletSpacingRel: 0.3,
    bulletOffetRel: -0.2,
    
    #Additional indentation for subblocks. Adds to bullets
    blockIndentationRel: 0.0,
    
    #Body properties
    bodyFont: Times,
    bodyFontColor: [0,0,0],    
    bodyFontSize: 9,
    bodyLineSpacingRel: 0.1,
    bodyParagraphSpacingRel: 0.1,
    
    #Fixed width (Courier) properties
    bodyFixedFontColor: [ 55, 92, 147 ],  
    bodyFixedFontSize: 9,
    
    blockSpacing: 4,
}